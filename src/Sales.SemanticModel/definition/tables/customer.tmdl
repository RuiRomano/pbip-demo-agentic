							"BirthDate",
							"MaritalStatus",
							"Gender",
							"YearlyIncome",
							"TotalChildren",
							"EnglishEducation",
							"EnglishOccupation"
						}),
						
						// Handle null values
						ReplaceNulls = Table.ReplaceValue(SelectColumns, null, "N/A", Replacer.ReplaceValue, {
							"FirstName", "LastName", "MaritalStatus", "Gender", "EnglishEducation", "EnglishOccupation"
						}),
						
						#"Changed Type" = Table.TransformColumnTypes(ReplaceNulls, {
							{"CustomerKey", Int64.Type},
							{"CustomerName", type text},
							{"FirstName", type text},
							{"LastName", type text},
							{"BirthDate", type date},
							{"MaritalStatus", type text},
							{"Gender", type text},
							{"YearlyIncome", type number},
							{"TotalChildren", Int64.Type},
							{"EnglishEducation", type text},
