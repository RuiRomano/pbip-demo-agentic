						AddCustomerName = Table.AddColumn(DimCustomer, "CustomerName", each 
							if [FirstName] <> null and [LastName] <> null 
							then [FirstName] & " " & [LastName] 
							else [CustomerAlternateKey]
						),
						
						// Select essential columns
						SelectColumns = Table.SelectColumns(AddCustomerName, {
							"CustomerKey",
